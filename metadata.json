{
  "name": "daenney-circus",
  "version": "0.1.0",
  "author": "Daniele Sluijters",
  "summary": "Circus process and socket manager",
  "license": "Apache 2.0",
  "source": "https://github.com/daenney/puppet-circus",
  "project_page": "https://github.com/daenney/puppet-circus",
  "issues_url": "https://github.com/daenney/puppet-circus/issues",
  "dependencies": [
  {
    "name": "puppetlabs-stdlib",
    "version_range": ">= 4.2.0"
  },
  {
    "name": "puppetlabs-inifile",
    "version_range": ">= 1.1.0",
  }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.2.0 < 3.4.0"
    },
    {
      "name": "puppet",
      "version_requirement": "3.x"
    }
  ],
  "operatingsystem_support": [
